{
  "name": "fastify-api-typescript-template",
  "version": "1.0.0",
  "description": "Axa fastify api template for typescript",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && copyfiles -f src/doc/* dist/doc && copyfiles -f src/database/migrations/*.js dist/database/migrations",
    "start": "node dist/run.js",
    "dev": "ts-node-dev src/run.ts | pino-pretty",
    "test": "dotenv -e .env.test -- vitest run --config vitest-spec.config.ts",
    "test:all": "dotenv -e .env.test -- vitest run --config vitest.all.config.ts --coverage",
    "test:watch": "dotenv -e .env.test -- vitest --config vitest-spec.config.ts",
    "test:e2e": "dotenv -e .env.test -- vitest run --coverage",
    "test:e2e:watch": "dotenv -e .env.test -- vitest",
    "lint": "eslint ./src/**/*.ts",
    "lint:pr": "./tools/lint-pr.sh dev",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/compress": "^6.1.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/helmet": "^10.0.2",
    "@fastify/multipart": "^7.7.1",
    "@fastify/static": "^6.6.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.1",
    "@fastify/type-provider-typebox": "^3.2.0",
    "@sinclair/typebox": "^0.28.9",
    "@types/http-errors": "^2.0.1",
    "@types/sequelize": "^4.28.14",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^5.3.1",
    "archiver-zip-encrypted": "^1.0.11",
    "axios": "^1.2.5",
    "cls-hooked": "^4.2.2",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "fastify": "^4.10.2",
    "fastify-metrics": "^10.0.0",
    "glob": "^8.1.0",
    "glob-promise": "^6.0.2",
    "http-errors": "^2.0.0",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.0",
    "msw": "^0.49.3",
    "mysql2": "^2.3.3",
    "pino": "^8.8.0",
    "qs": "6.11.1",
    "rollup-plugin-swc": "^0.2.1",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "sql-template-strings": "^2.2.2",
    "sqlite3": "^5.1.6",
    "umzug": "^3.2.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.9.0",
    "@faker-js/faker": "^8.0.2",
    "@types/archiver": "^5.3.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@vitest/coverage-c8": "^0.25.2",
    "@vitest/coverage-istanbul": "^0.27.1",
    "commander": "^11.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-formatter-json": "^8.40.0",
    "eslint-formatter-stylish": "^8.40.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.3",
    "pino-pretty": "^10.2.0",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3",
    "vitest": "^0.27.1"
  }
}
